{
  "name": "omniscript-cli",
  "version": "0.5.1",
  "description": "OmniScript Format (OSF) CLI tools",
  "main": "dist/osf.js",
  "bin": {
    "osf": "bin/osf.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "dev": "tsc -p tsconfig.json --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit -p tsconfig.json"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "omniscript-parser": "^0.5.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.31",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "osf",
    "cli",
    "document",
    "parser"
  ],
  "author": "Alphin Tom",
  "license": "MIT"
}
